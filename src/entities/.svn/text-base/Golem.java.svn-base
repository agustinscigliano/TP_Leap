package entities;

/**
 * Contains the Golem specifications.
 * @author mpurita,gdelgiud,ppauli
 */

public class Golem extends Enemy{
    /**
	 * Constructs a Golem and sets its attributes according to its level.
	 * @param level - Integer
	 */
    public Golem(int level) throws InvalidLevelException{
        super(level);
        setName("Golem");
        setAttributeConstants(1.0, 0.7);
        reset();
    }
    
    @Override
    public int getFileDescription() {
            return super.getFileDescription();
    }

    public int getEnemyType() {
            return 1;
    }
    
    public int getLevel(){
    	return this.level;
    }
}

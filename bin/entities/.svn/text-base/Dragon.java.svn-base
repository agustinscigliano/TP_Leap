package entities;

/**
 * Contains the dragon specifications
 * @author mpurita,gdelgiud,ppauli
 */
public class Dragon extends Enemy{

    /**
	 * Constructs a Dragon and sets its attributes according to its level.
	 * @param level - Integer
	 */
    public Dragon(int level) throws InvalidLevelException{
        super(level);
        setName("Dragon");
        setAttributeConstants(1.35, 1.0);
        reset();
    }

    @Override
    public int getFileDescription() {
            return super.getFileDescription();
    }

    public int getEnemyType() {
            return 2;
    }
    public int getLevel(){
    	return this.level;
    }
}
